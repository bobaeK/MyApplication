<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/window3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_weight="1"
    android:visibility="gone">



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:paddingTop="5dp"
        android:paddingLeft="3dp"
        android:paddingRight="3dp"
        android:paddingBottom="5dp">

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.3dp"
            android:background="@color/underLine" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1.2"
            android:background="@color/setBold"
            android:gravity="left|center"
            android:text="@string/txt1"
            style="?android:attr/borderlessButtonStyle"
            android:textColor="@color/txtMain" />

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.3dp"
            android:background="@color/underLine" />

        <Switch
            android:id="@+id/bluetooth_switch"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:padding="10dp"
            android:background="@drawable/border"
            android:text="Bluetooth"
            android:textColor="@color/txtSub"/>
        <Switch
            android:id="@+id/gps_switch"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="@drawable/border"
            android:padding="10dp"
            android:text="GPS"
            android:textColor="@color/txtSub"/>
        <!--개인설정-->

        <TextView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1.2"
            android:background="@color/setBold"
            android:gravity="left|center"
            android:text="@string/txt2"
            style="?android:attr/borderlessButtonStyle"
            android:textColor="@color/txtMain" />

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.3dp"
            android:background="@color/underLine" />

        <Button
            android:id="@+id/txt2btn1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="#ffffff"
            android:gravity="left|center"
            android:text="@string/txt2ch1"
            android:textColor="@color/txtSub" />

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.3dp"
            android:background="@color/underLine" />

        <Button
            android:id="@+id/txt2btn2"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="#ffffff"
            android:gravity="left|center"
            android:text="@string/txt2ch2"
            android:textColor="@color/txtSub" />

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.3dp"
            android:background="@color/underLine" />


        <TextView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1.2"
            android:background="@color/setBold"
            android:gravity="left|center"
            android:text="@string/txt3"
            style="?android:attr/borderlessButtonStyle"
            android:textColor="@color/txtMain" />

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.3dp"
            android:background="@color/underLine" />

        <Button
            android:id="@+id/txt3btn1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="#ffffff"
            android:gravity="left|center"
            android:text="@string/txt3ch1"
            android:textColor="@color/txtSub"
            android:onClick="onMapIconClick"/>

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.3dp"
            android:background="@color/underLine" />

        <Button
            android:id="@+id/txt3btn2"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="#ffffff"
            android:gravity="left|center"
            android:text="@string/txt3ch2"
            android:textColor="@color/txtSub"
            android:onClick="onMapIconClick"/>

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.3dp"
            android:background="@color/underLine" />

        <Button
            android:id="@+id/txt3btn3"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="#ffffff"
            android:gravity="left|center"
            android:text="@string/txt3ch3"
            android:textColor="@color/txtSub"
            android:onClick="onMapIconClick"/>

        <View
            android:layout_width="wrap_content"
            android:layout_height="0.3dp"
            android:background="@color/underLine" />

        <Button
            android:id="@+id/txt3btn4"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:background="#ffffff"
            android:gravity="left|center"
            android:text="@string/txt3ch4"
            android:textColor="@color/txtSub"
            android:onClick="onMapIconClick"/>
        <View
            android:layout_width="wrap_content"
            android:layout_height="0.3dp"
            android:background="@color/underLine" />
    </LinearLayout>


</FrameLayout>